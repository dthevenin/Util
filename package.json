{
  "name": "vs_utils",
  "version": "0.0.1",
  "description": "Set of utilities functions we need for developing web app.",
  "repository": "https://github.com/dthevenin/Util.git",
  "author": "<david@thevenin.jp>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist build",
    "prepublish": "npm run clean && npm run build",
    "tsc": "tsc -p src/tsconfig.json",
    "make-bundle": "rollup --config",
    "build": "npm run clean; npm run tsc; npm run make-bundle",
    "test": "nyc mocha test/*.js test/**/*.js  test/**/**/*.js"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^18.14.6",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.27.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/vs_utils.es.js",
      "require": "./dist/vs_utils.amd.js",
      "types": "./dist/vs_utils.d.js"
    }
  },
  "type": "module"
}
